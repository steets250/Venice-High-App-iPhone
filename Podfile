  pod 'RFAboutView-Swift', :git => 'https://github.com/steets250/RFAboutView-Swift.git'
  pod 'Solar'
  pod 'Spruce'
  pod 'SwiftMapVC', :git => 'https://github.com/steets250/SwiftMapVC.git'
  pod 'SwiftTheme'
  pod 'SwiftWebVC'
  pod 'SwipeCellKit'
end

target 'Bell Schedule' do
  # Pods for Bell Schedule
